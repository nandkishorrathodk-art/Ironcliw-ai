# Default: Cmd+Shift+J | Usage: 2 files
KEYBOARD_SHORTCUT=Cmd+Shift+J

# Enable button fallback
# Default: true | Usage: 2 files
ENABLE_BUTTON_FALLBACK=true

# ============================================================================
# BUILD & COMPILATION (Usually set by system)
# ============================================================================

# Cargo incremental compilation
# Default: 1 | Usage: 2 files
CARGO_INCREMENTAL=1

# Cargo build jobs (parallel builds)
# Default: 4 | Usage: 3 files
CARGO_BUILD_JOBS=4

# Cargo terminal progress mode
# Default: auto | Usage: 1 file
CARGO_TERM_PROGRESS_WHEN=auto

# Cargo terminal progress width
# Default: 80 | Usage: 1 file
CARGO_TERM_PROGRESS_WIDTH=80

# C++ compiler flags
# Default: -O2 | Usage: 1 file
CXXFLAGS=-O2

# ============================================================================
# SYSTEM ENVIRONMENT (Usually set by system)
# ============================================================================

# Python: Don't write bytecode files
# Default: 1 | Usage: 7 files
PYTHONDONTWRITEBYTECODE=1

# Python: Unbuffered output
# Default: 1 | Usage: 6 files
PYTHONUNBUFFERED=1

# Python: Module search path
# Default: /path/to/jarvis | Usage: 8 files
PYTHONPATH=/path/to/jarvis

# Dynamic linker library path (macOS)
# Default: /usr/local/lib | Usage: 7 files
DYLD_LIBRARY_PATH=/usr/local/lib

# Dynamic linker library path (Linux)
# Default: /usr/local/lib | Usage: 2 files
LD_LIBRARY_PATH=/usr/local/lib

# Objective-C: Disable fork safety (macOS)
# Default: YES | Usage: 3 files
OBJC_DISABLE_INITIALIZE_FORK_SAFETY=YES

# User name (system variable)
# Default: username | Usage: 8 files
USER=username

# Display server (X11/Wayland)
# Default: :0 | Usage: 1 file
DISPLAY=:0

# Browser executable
# Default: /usr/bin/firefox | Usage: 1 file
BROWSER=/usr/bin/firefox

# SSH client IP address
# Default: 192.168.1.1 | Usage: 1 file
SSH_CLIENT=192.168.1.1

# SSH TTY device
# Default: /dev/pts/0 | Usage: 1 file
SSH_TTY=/dev/pts/0

# Node.js options
# Default: --max-old-space-size=4096 | Usage: 2 files
NODE_OPTIONS=--max-old-space-size=4096

# Skip preflight check (React/Node)
# Default: true | Usage: 2 files
SKIP_PREFLIGHT_CHECK=true

# Generate source maps (React/Node)
# Default: false | Usage: 1 file
GENERATE_SOURCEMAP=false

# ============================================================================
# END OF CONFIGURATION
# ============================================================================
