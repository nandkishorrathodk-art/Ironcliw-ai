# ============================================================================
# JARVIS AI Agent - Complete Environment Configuration
# ============================================================================
# This file contains ALL 701 environment variables used by the JARVIS AI Agent.
# Copy this file to .env and configure the values you need for your setup.
# ============================================================================

# ============================================================================
# CORE CONFIGURATION - Database & Cloud Services
# ============================================================================

# Database type: "cloudsql" for Google Cloud SQL, "sqlite" for local database
# Default: cloudsql | Usage: Core database configuration
JARVIS_DB_TYPE=cloudsql

# Cloud SQL connection name (format: project:region:instance)
# Default: your-project:region:instance-name | Usage: Cloud SQL connection
JARVIS_DB_CONNECTION_NAME=your-project:region:instance-name

# Database host (127.0.0.1 for Cloud SQL proxy, or Cloud SQL private IP)
# Default: 127.0.0.1 | Usage: Database connection
JARVIS_DB_HOST=127.0.0.1

# Database port
# Default: 5432 | Usage: PostgreSQL default port
JARVIS_DB_PORT=5432

# Database name
# Default: jarvis | Usage: Database name
JARVIS_DB_NAME=jarvis

# Database user
# Default: postgres | Usage: Database user
JARVIS_DB_USER=postgres

# Database password
# Default: your-database-password | Usage: Database authentication
JARVIS_DB_PASSWORD=your-database-password

# GCP project ID
# Default: your-gcp-project-id | Usage: GCP resource identification
GCP_PROJECT_ID=your-gcp-project-id

# GCP region
# Default: us-central1 | Usage: GCP resource location
GCP_REGION=us-central1

# GCP zone
# Default: us-central1-a | Usage: GCP resource availability zone
GCP_ZONE=us-central1-a

# GCP default zone
# Default: us-central1-a | Usage: 2 files
GCP_DEFAULT_ZONE=us-central1-a

# Enable/disable GCP Spot VM auto-creation
# Default: true | Usage: VM management
GCP_VM_ENABLED=true

# GCP VM type
# Default: e2-medium | Usage: 4 files
GCP_VM_TYPE=e2-medium

# GCP VM daily budget in USD
# Default: 10.00 | Usage: 4 files
GCP_VM_DAILY_BUDGET=10.00

# Maximum VM lifetime in hours
# Default: 24 | Usage: 2 files
MAX_VM_LIFETIME_HOURS=24

# Regular VM hourly cost
# Default: 0.05 | Usage: 2 files
REGULAR_VM_HOURLY_COST=0.05

# Spot VM hourly cost
# Default: 0.015 | Usage: 2 files
SPOT_VM_HOURLY_COST=0.015

# Orphaned VM max age in hours before cleanup
# Default: 48 | Usage: 2 files
ORPHANED_VM_MAX_AGE_HOURS=48

