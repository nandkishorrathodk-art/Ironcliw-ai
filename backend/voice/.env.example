# Voice System Environment Variables

# Picovoice Configuration
# Get your free key at https://console.picovoice.ai/
PICOVOICE_ACCESS_KEY=your-key-here

# Voice Detection Settings
WAKE_WORD_THRESHOLD=0.55
CONFIDENCE_THRESHOLD=0.6
ENABLE_VAD=true
ENABLE_STREAMING=true
USE_PICOVOICE=true

# Optimization Settings
Ironcliw_OPTIMIZATION_LEVEL=balanced
Ironcliw_MAX_MEMORY_PERCENT=25
Ironcliw_MAX_MODELS_IN_MEMORY=3

# macOS Acceleration
Ironcliw_USE_COREML=true
Ironcliw_USE_METAL=true

# Debug Settings
Ironcliw_DEBUG_MODE=false
Ironcliw_LOG_PERFORMANCE_METRICS=true